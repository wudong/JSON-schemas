{
	"title": "Sequence schema",
	"type": "object",
	"properties": {
		"id": {
			"type": "string",
			"pattern": "(^([O,P,Q][0-9][A-Z,0-9]{3}[0-9])(-(\\d+))?$)|(^([A-N,R-Z]([0-9][A-Z][A-Z,0-9]{2}){1,2}[0-9])(-(\\d+))?$)|(^\\w+_\\w+$)"
		},
		"start": {
			"type": "integer",
			"minimum": 1,
			"maximum": 1
		},
		"stop": {
			"type": "integer",
			"minimum": 1
		},
		"version": {
			"type": "string"
		},
		"label": {
			"type": "string"
		},
		"sequence": {
			"type": "string"
		}
	},
	"required": ["id", "start","stop","version","label","sequence"]
}
