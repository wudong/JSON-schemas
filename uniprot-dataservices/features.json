{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"id": "http://jsonschema.net",
	"type": "object",
	"properties": {
		"accession": {
			"type": "string",
			"pattern": "(^([OPQ][0-9][A-Z0-9]{3}[0-9])(-(\\d+))?$)|(^([A-NR-Z]([0-9][A-Z][A-Z0-9]{2}){1,2}[0-9])(-(\\d+))?$)"
		},
		"entryName": {
			"type": "string",
			"pattern": "(^\\w+_\\w+$)"
		},
		"sequence": {
			"type": "string"
		},
		"sequenceVersion": {
			"type": "string"
		},
		"eGeneId": {
			"type": "string"
		},
		"eProteinId": {
			"type": "string"
		},
		"features": {
			"type": "array",
			"minItems": 0,
			"items": {
				"type": "object",
				"$ref": "#/definitions/featureObject"
			}
		},
		"variants": {
			"type": "array",
			"minItems": 0,
			"items": {
				"type": "object",
				"$ref": "#/definitions/variantObject"
			}
		}
	},
	"required": ["accession", "entryName", "sequence", "sequenceVersion", "features", "variants"],
	"definitions": {
		"featureObject": {
			"properties": {
				"categoryAbbrev": {
					"type": "string",
					"pattern": "^(MOL_PROC|STR_FT|DOM_SITE|PTM|MUTA|SEQ_INFO|TOPO)$"
				},
				"typeAbbrev": {
					"type": "string",
					"pattern": "^(INIT_MET|SIGNAL|PROPEP|TRANSIT|CHAIN|PEPTIDE|TURN|HELIX|STRAND|CROSSLNK|DISULFID|DOMAIN|REPEAT|CA_BIND|ZN_FING|DNA_BIND|NP_BIND|REGION|COILED|MOTIF|ACT_SITE|METAL|BINDING|SITE|MOD_RES|LIPID|CARBOHYD|MUTAGEN|COMPBIAS|CONFLICT|NON_CONS|NON_TER|UNSURE|NON_STD|TOPO_DOM|TRANSMEM|INTRAMEM)$"
				},
				"begin": {
					"type": "integer",
					"minimum": 1
				},
				"end": {
					"type": "integer",
					"minimum": 1
				},
				"description": {
					"type": "string"
				},
				"sourceIds": {
					"type": "array",
					"minItems": 1,
					"items": {
						"type": "object",
						"$ref": "#/definitions/linkObject"
					},
					"uniqueItems": true
				},
				"originalSequence": {
					"type": "string"
				},
				"alternativeSequence": {
					"type": "string"
				},
				"evidence": {
					"type": "array",
					"items": {
						"type": "object",
						"$ref": "#/definitions/evidenceObject"
					},
					"minItems": 0
				}
			},
			"required": ["categoryAbbrev", "typeAbbrev", "begin"]
		},
		"variantObject": {
			"properties": {
				"begin": {
					"type": "integer",
					"minimum": 1
				},
				"cytogeneticBand": {
					"type": "string"
				},
				"genomicLocation": {
					"type": "string"
				},
				"mutation": {
					"type": "array",
					"items": {
						"type": "object",
						"$ref": "#/definitions/mutationObject"
					},
					"minItems": 1
				}
			},
			"required": ["begin", "mutation"]
		},
		"mutationObject": {
			"properties": {
				"typeAbbrev": {
					"type": "string",
					"pattern": "^(MS_DIS|MS_DEL|MS_INT|MS_BEN|MS_UNK|STOP_GAINED|STOP_LOST|INSDEL|INIT_CODON)$"
				},
				"length": {
					"type": "integer",
					"minimum": 2
				},
				"mutatedType": {
					"type": "string",
					"pattern": "^(A|R|N|D|C|Q|E|G|H|I|L|K|M|F|P|S|T|W|Y|V|-|\\*)+$"
				},
				"frequency": {
					"type": "number",
					"minimum": 0,
					"maximum": 1.0
				},
				"polyphenPrediction": {
					"type": "string"
				},
				"polyphenScore": {
					"type": "number",
					"minimum": 0,
					"maximum": 1.0
				},
				"siftPrediction": {
					"type": "string"
				},
				"siftScore": {
					"type": "number",
					"minimum": 0,
					"maximum": 1.0
				},
				"somaticStatus": {
					"type": "integer",
					"minimum": 0,
					"maximum": 1
				},
				"association": {
					"type": "string"
				},				
				"clinicalSignificances": {
					"type": "array",
					"minItems": 1,
					"items": {
						"type": "string"
					},
					"uniqueItems": true
				},
				"description": {
					"type": "string"
				},
				"sourceIds": {
					"type": "array",
					"minItems": 1,
					"items": {
						"type": "object",
						"$ref": "#/definitions/linkObject"
					},
					"uniqueItems": true
				},
				"evidence": {
					"type": "array",
					"items": {
						"type": "object",
						"$ref": "#/definitions/evidenceObject"
					},
					"minItems": 0
				}
			}, 
			"required": ["mutatedType"]
		},
		"linkObject": {
			"properties": {
				"reference": {
					"type": "string"
				},
				"url": {
					"type": "string"
				}
			},
			"required": ["reference"]
		}, 
		"evidenceObject": {
			"properties": {
				"type": {
					"type": "string",
					"pattern": "^ECO:[0-9]{7}$"
				},
				"source": {
					"type": "object",
					"oneOf": [
						{
							"$ref": "#/definitions/dbReferenceObject"
						}, {
							"$ref": "#/definitions/linkObject"
						}
					]
				}
			},
			"required": ["type", "source"]
		},
		"dbReferenceObject": {
			"properties": {
				"dbReferenceType": {
					"type": "string"
				},
				"dbReferenceId": {
					"type": "string"
				},
				"url": {
					"type": "string"
				}
			},
			"required": ["dbReferenceType", "dbReferenceId"]
		}
	}
}
