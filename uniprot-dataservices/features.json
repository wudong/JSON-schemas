{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"id": "http://jsonschema.net",
	"type": "object",
	"properties": {
		"accession": {
			"type": "string",
			"pattern": "(^([O,P,Q][0-9][A-Z,0-9]{3}[0-9])(-(\\d+))?$)|(^([A-N,R-Z]([0-9][A-Z][A-Z,0-9]{2}){1,2}[0-9])(-(\\d+))?$)"
		},
		"entryName": {
			"type": "string",
			"pattern": "(^\\w+_\\w+$)"
		},
		"sequence": {
			"type": "string"
		},
		"sequenceVersion": {
			"type": "string"
		},
		"eGeneId": {
			"type": "string"
		},
		"eProteinId": {
			"type": "string"
		},
		"features": {
			"type": "array",
			"minItems": 0,
			"items": {
				"type": "object",
				"$ref": "#/definitions/featureObject"
			}
		}
	},
	"required": ["accession", "entryName", "sequence", "sequenceVersion", "features"],
	"definitions": {
		"featureObject": {
			"properties": {
				"category": {
					"type": "string",
					"pattern": "^(MOLECULE PROC\\.|STRUCTURAL FT\\.|DOMAINS & SITES|POST TRANSLATIONAL MODIFICATIONS|MUTAGENEIS|SEQUENCE INFO\\.|TOPOLOGY|VARIANTS)$"
				},
				"categoryAbbrev": {
					"type": "string",
					"pattern": "^(MOL_PROC|STR_FT|DOM_SITE|PTM|NAT_VAR|MUTA|SEQ_INFO|TOPO)$"
				},
				"type": {
					"type": "string",
					"pattern": "^(INIT. METHIONINE|SIGNAL|PROPEPTIDE|TRANSIT|CHAIN|PEPTIDE|TURN|HELIX|BETA STRAND|CROSSLNK|DISULFIDE BOND|DOMAIN|REPEAT|CALCIUM BINDING|ZINC FINGER|DNA BIND|NUCLEOTIDE BIND|REGION OF INTEREST|COILED COIL|SHORT SEQ. MOTIF|ACTIVE SITE|METAL ION-BINDING|BINDING|SITE|MODIFIED RESIDUE|LIPID|GLYCOSILATION|MUTAGENESIS|COMPOSITIONAL BIAS|CONFLICT|NON CONSECUTIVE|NON TERMINAL|UNSURE SEQ.|NON STANDARD|TOPOLOGICAL DOMAIN|TRANSMEMBRANE|INTRAMEMBRANE|MISSENSE|INSERT & DELETE|STOP GAINED|STOP LOST|INITIATOR CODON)$"
				},
				"typeAbbrev": {
					"type": "string",
					"pattern": "^(INIT_MET|SIGNAL|PROPEP|TRANSIT|CHAIN|PEPTIDE|TURN|HELIX|STRAND|CROSSLNK|DISULFID|DOMAIN|REPEAT|CA_BIND|ZN_FING|DNA_BIND|NP_BIND|REGION|COILED|MOTIF|ACT_SITE|METAL|BINDING|SITE|MOD_RES|LIPID|CARBOHYD|MUTAGEN|COMPBIAS|CONFLICT|NON_CONS|NON_TER|UNSURE|NON_STD|TOPO_DOM|TRANSMEM|INTRAMEM|MISSENSE|INDEL|STOP_GAINED|STOP_LOST|INIT_CODON)$"
				},
				"cvId": {
					"type": "string"
				},
				"ftId": {
					"type": "string",
					"unique": true
				},
				"sourceIds": {
					"type": "array",
					"minItems": 1,
					"items": {
						"type": "string"
					},
					"uniqueItems": true
				},
				"description": {
					"type": "string"
				},
				"begin": {
					"type": "integer",
					"minimum": 1
				},
				"end": {
					"type": "integer",
					"minimum": 1
				},
				"status": {
					"type": "string"
				},
				"evidence": {
					"type": "array",
					"items": {
						"type": "object",
						"$ref": "#/definitions/evidenceObject"
					},
					"minItems": 1
				},				
				"variant": {
					"type": "object",
					"$ref": "#/definitions/variantObject"
				}
			},
			"required": ["category", "type", "ftId", "begin", "end"]
		},
		"variantObject": {
			"properties": {
				"wildType": {
					"type": "string",
					"pattern": "^(A|R|N|D|C|Q|E|G|H|I|L|K|M|F|P|S|T|W|Y|V|\\*)$"
				},
				"mutation": {
					"type": "string",
					"pattern": "^(A|R|N|D|C|Q|E|G|H|I|L|K|M|F|P|S|T|W|Y|V|-|\\*)+$"
				},
				"frequency": {
					"type": "number",
					"minimum": 0,
					"maximum": 1.0
				},
				"polyphenPrediction": {
					"type": "string"
				},
				"polyphenScore": {
					"type": "number",
					"minimum": 0,
					"maximum": 1.0
				},
				"siftPrediction": {
					"type": "string"
				},
				"siftScore": {
					"type": "number",
					"minimum": 0,
					"maximum": 1.0
				},
				"somaticStatus": {
					"type": "integer",
					"minimum": 0,
					"maximum": 1
				},
				"cytogeneticBand": {
					"type": "string"
				},
				"genomicLocation": {
					"type": "string"
				},
				"association": {
					"type": "string"
				},				
				"clinicalSignificances": {
					"type": "array",
					"minItems": 1,
					"items": {
						"type": "string"
					},
					"uniqueItems": true
				}
			},
			"required": ["wildType", "mutation", "frequency", "polyphenPrediction", "polyphenScore", "siftPrediction", "siftScore"]
		},
		"evidenceObject": {
			"properties": {
				"type": {
					"type": "string",
					"pattern": "^ECO:[0-9]{7}$"
				},
				"source": {
					"type": "object",
					"oneOf": [
						{
							"$ref": "#/definitions/dbReferenceObject"
						}, {
							"$ref": "#/definitions/referenceObject"
						}
					]
				}
			},
			"required": ["type", "source"]
		},
		"dbReferenceObject": {
			"properties": {
				"dbReferenceType": {
					"type": "string"
				},
				"dbReferenceId": {
					"type": "string"
				}
			},
			"required": ["dbReferenceType", "dbReferenceId"]
		},
		"referenceObject": {
			"properties": {
				"ref": {
					"type": "string"
				}
			},
			"required": ["ref"]
		}
	}
}
